<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.backslashtrash.project1.Controller">
   <children>
      <HBox VBox.vgrow="ALWAYS" />
       <HBox alignment="BOTTOM_CENTER" VBox.vgrow="ALWAYS">
         <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
            <children>
               <Text fill="#4a93ff" strokeType="OUTSIDE" strokeWidth="0.0" text="Management Engine" textAlignment="CENTER">
                  <font>
                     <Font name="Segoe UI Bold" size="26.0" />
                  </font>
               </Text>
               <HBox alignment="BOTTOM_CENTER" prefHeight="100.0" prefWidth="200.0" />
               <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#onRegister" prefHeight="70.0" prefWidth="122.0" text="Start" textAlignment="RIGHT">
                  <font>
                     <Font name="SwedenSans-Regular" size="23.0" />
                  </font>
               </Button>
            </children>
         </VBox>
       </HBox>
      <HBox VBox.vgrow="ALWAYS" />
   </children>
</VBox>
